apiVersion: networking.k8s.io/v1
kind: Ingress
metadata:
  name: sravyaops
  annotations:
    nginx.ingress.kubernetes.io/rewrite-target: /
spec:
  ingressClassName: nginx
  tls:
  - hosts:
    - kuttysravya.shop
    secretName: awscloudops
  rules:
  - host: kuttysravya.shop
    http:
      paths:
      - path: /
        pathType: Prefix
        backend:
          service:
            name: webapp
            port:
              number: 80

server {
        listen 80;
        server_name kuttysravya.shop www.kuttysravya.shop;
        return 301 https://$server_name$request_uri;
        location / {
                proxy_pass http://localhost;
                include proxy_params;
        }
}

server {
        listen 443 ssl;
        server_name kuttysravya.shop www.kuttysravya.shop;

        ssl_certificate /etc/letsencrypt/live/kuttysravya.shop/fullchain.pem;
        ssl_certificate_key /etc/letsencrypt/live/kuttysravya.shop/privkey.pem;

        location / {
                root /var/www/html;
                index index.html index.htm;
        }
}


